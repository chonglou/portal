$!{extends("/layout.httl")}
#macro(main)
<!--#set(List<User> userList)-->
<!--#set(List<Tag> tagList)-->
<!--#set(List<Article> articleList)-->
<div class="col-lg-12">
    <br>
    <h3>用户列表</h3>
    <hr>
    <div class="row">
        #for(u : userList)
        <div class="col-lg-1">
            <a href="/user/${u.id}" target="_blank">${u.username}</a>
        </div>
        #end
    </div>
    <p>&nbsp;</p>
    <h3>标签列表</h3>
    <hr>
    <div class="row">
        #for(t : tagList)
        <div class="col-lg-1">
            <a href="/tag/${t.id}" target="_blank">${t.name}</a>
        </div>
        #end
    </div>
    <p>&nbsp;</p>
    <h3>文章列表</h3>
    <hr>
    ${listArticle}
    <p>&nbsp;</p>
</div>
#end
